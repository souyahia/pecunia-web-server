{
  "name": "pecunia-web-server",
  "version": "0.0.1",
  "description": "An Express web server for the Pecunia app.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Samy Ouyahia",
  "email": "sr.ouyahia@gmail.com",
  "url": "",
  "keywords": [
    "pecunia",
    "web",
    "server",
    "typescript",
    "express"
  ],
  "license": "ISC",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "pretest": "npm run build",
    "test": "jest",
    "posttest": "npm run lint",
    "pretest:coverage": "npm run build",
    "test:coverage": "jest --coverage",
    "posttest:coverage": "npm run lint",
    "clean": "rimraf dist && mkdirp dist",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ./test",
    "eslint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ./test",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "prettier:fix": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\" \"test/**/*.{ts,tsx}\"",
    "prestart": "npm run build",
    "start": "node dist/server.js | ./node_modules/.bin/bunyan",
    "start:watch": "nodemon"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.8",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.6.4",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.2.2",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "bunyan": "^1.8.14",
    "bunyan-debug-stream": "^2.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "nconf": "^0.10.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.25"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/souyahia/pecunia-web-server.git"
  },
  "private": false
}
